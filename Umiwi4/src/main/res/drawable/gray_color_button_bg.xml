<?xml version="1.0" encoding="utf-8"?>
<!-- 空心圆角矩形 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners android:radius="5dp" /> <!-- 圆角半径 -->
    <size android:height="30dp"
        android:width="60dp"></size>
    <stroke
        android:width="1px"
        android:color="@color/line" /> <!-- 矩形的边线 -->
    <solid android:color="@color/line" />
    <!--<gradient-->
        <!--android:endColor="@color/umiwi_white"-->
        <!--android:startColor="@color/umiwi_white" /> &lt;!&ndash; 矩形的内部颜色，这里设置成白色，即空心 &ndash;&gt;-->
    <padding
        android:bottom="5dp"
        android:left="7dp"
        android:right="7dp"
        android:top="5dp" />
</shape>